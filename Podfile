  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SunriseAPIDemo WatchKit App
  pod 'Alamofire', '~> 5.0.0-rc.2'
  pod 'SwiftyJSON', '~> 4.0'
end

target 'SunriseAPIDemo WatchKit Extension' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!4

  # Pods for SunriseAPIDemo WatchKit Extension
  pod 'Alamofire', '~> 5.0.0-rc.2'
